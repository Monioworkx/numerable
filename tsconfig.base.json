    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "lib": ["dom", "es7"],

        "declaration": true,
        "sourceMap": true,
        "allowSyntheticDefaultImports": true,
    },
}